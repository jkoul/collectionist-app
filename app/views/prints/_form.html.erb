<!-- Create the basic form. -->
<!-- Create the table column headers input fields. -->

<%= form_for @print do |f| %>
  <%= f.label :photo %>
  <%= f.text_field :photo %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :creator_individual %>
  <%= f.text_field :creator_individual %>

  <%= f.label :creator_company %>
  <%= f.text_area :creator_company %>

  <%= f.label :series %>
  <%= f.text_field :series %>

  <%= f.label :purchase_year %>
  <%= f.text_field :purchase_year %>

  <%= f.label :purchase_price %>
  <%= f.text_field :purchase_price %>

  <%= f.submit %>
<% end %>

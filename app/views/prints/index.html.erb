<div class="grid-column">
<!-- if current_user.print.count == 0 -->
  
<!-- else -->
  <% current_user.prints.each do |print| %>
    <div class="grid-item">
      <%= link_to image_tag(print.photo.to_s), print_path(print) %>
      <h1><%= print.title %></h1>
      <h2><%= print.creator_individual %></h2>
    </div>
  <% end %>
</div>